        </activity>
        <activity android:name=".BebidasActivity" />
        <activity android:name=".PeixesActivity" />
        <activity android:name=".CarnesActivity" />
        <activity android:name=".CarboidratosActivity" />
        <activity android:name=".FrutaActivity" />
        <activity android:name=".VegetaisActivity" />
